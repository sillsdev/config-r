{
  "name": "@sillsdev/config-r",
  "license": "MIT",
  "packageManager": "yarn@3.4.1",
  "description": "A react library for a settings UI similar to that of Chrome & Edge.",
  "keywords": [
    "react",
    "settings",
    "preferences"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BloomBooks/config-r"
  },
  "scripts": {
    "dev": "start http://localhost:3000 && vite",
    "// see https://github.com/vitejs/vite/issues/3461, this uses a totally different builder that has its typescript typings act more together than vitejs": "",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "formik": "^2.2.9",
    "formik-mui": "^4.0.0-alpha.3",
    "lodash": "^4.17.21",
    "mark.js": "^8.11.1",
    "react-children-utilities": "^2.6.3",
    "react-json-view": "^1.21.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.5.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@emotion/babel-plugin": "^11.7.2",
    "@emotion/eslint-plugin": "^11.10.0",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.5.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.0",
    "@storybook/react": "^6.5.16",
    "@types/lodash": "^4.14.178",
    "@types/mark.js": "^8.11.7",
    "@types/react": "^17.0.43",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.10",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.4",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2"
  },
  "pre-commit": "lint",
  "files": [
    "dist"
  ],
  "main": "./dist/configr.es.js",
  "module": "./dist/configr.es.js",
  "//about index.d.ts": "see comment in vite.config.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/configr.es.js",
      "require": "./dist/configr.es.js"
    }
  }
}
